{
  "parser": "@typescript-eslint/parser",
  "extends": ["next/core-web-vitals", "prettier", "plugin:import/recommended", "plugin:import/typescript"],
  "plugins": ["prettier", "@typescript-eslint"],
  "rules": {
    "import/order": [
      "error",
      {
        "newlines-between": "always",
        "alphabetize": {
          "order": "asc",
          "caseInsensitive": false
        }
      }
    ]
  }
}
